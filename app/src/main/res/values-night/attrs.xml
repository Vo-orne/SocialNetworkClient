<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="colorFirstBackground" format="color"/>
    <attr name="colorSecondBackground" format="color"/>
    <attr name="colorMainText" format="color"/>
    <attr name="colorSecondMainText" format="color"/>
    <attr name="colorSecondaryText" format="color"/>
    <attr name="colorSecondaryTextForContacts" format="color"/>
    <attr name="colorOfDataEntryFields" format="color"/>
    <attr name="colorOfDataEntryFieldsForAddContact" format="color"/>
    <attr name="colorTextForAddContact" format="color"/>
    <attr name="colorHintTextForAddContact" format="color"/>
    <attr name="colorAccentButtons" format="color"/>
    <attr name="colorGoogleButton" format="color"/>
    <attr name="colorGoogleButtonText" format="color"/>

    <declare-styleable name="GoogleButton">
        <attr name="buttonColor" format="color" />
        <attr name="icon" format="reference" />
        <attr name="iconTint" format="color" />
        <attr name="iconGravityX" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
            <enum name="textStart" value="2" />
            <enum name="textEnd" value="3" />
        </attr>
        <attr name="iconSizeX" format="dimension" />
        <attr name="text" format="string" />
        <attr name="android:textSize" />
        <attr name="android:textAllCaps" />
        <attr name="android:textColor" />
        <attr name="buttonBackground" format="reference" />
    </declare-styleable>

</resources>
